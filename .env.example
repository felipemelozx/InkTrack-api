DOCKER_USERNAME=seu_usuario_dockerhub
IMAGE_TAG=latest

APP_PORT=8081
SPRING_PROFILES_ACTIVE=prod
SERVER_CONTEXT_PATH=/api/v1

API_SECRET_KEY=troque-esta-chave-por-uma-muito-segura

FRONT_URL=http://localhost:3000

POSTGRES_USER=inktrack_user
POSTGRES_PASSWORD=inktrack_password
POSTGRES_DB=inktrack_db

# Google Books API Configuration
# API key is optional but recommended for higher quotas and better rate limiting
# Get your free API key at: https://console.cloud.google.com/apis/credentials
GOOGLE_BOOKS_API_KEY=
GOOGLE_BOOKS_MAX_RESULTS=10
GOOGLE_BOOKS_TIMEOUT_MS=5000
# Cache Configuration (Caffeine - in-memory cache)
# Maximum number of search results to cache
GOOGLE_BOOKS_CACHE_MAX_SIZE=100
# Cache expiration time in minutes
GOOGLE_BOOKS_CACHE_EXPIRATION_MINUTES=5
